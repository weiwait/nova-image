{
    "name": "weiwait/nova-image",
    "description": "A Laravel Nova field. Laravel Nova 图片拖拽上传、裁剪组件",
    "keywords": [
        "laravel", "nova","image", "images", "drop", "drag", "拖拽", "裁剪", "nova组件"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Weiwait\\NovaImage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weiwait\\NovaImage\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
